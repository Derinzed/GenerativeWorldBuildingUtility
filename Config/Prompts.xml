<Prompts>
  <prompt name="template">
    <ValidModifiers value="TestModifier,TestModifier2"/>
    <PromptLine id="1" filter="" value="">
      <PrerequisitePrompts> 
        <PrereqPrompt prompt="" returnName="" number=""/>
      </PrerequisitePrompts>
      <RandomData>
        <DataElement DataList="" number ="" repeatable="" returnName=""/>
      </RandomData>
    </PromptLine>
  </prompt>
  <prompt name="Fantasy Scene">
    <ValidModifiers value=""/>
    <PromptLine id="1" value="Describe to me a fantasy scene, in great detail.  The scene is of a @LocationType@">
      <PrerequisitePrompts>
      </PrerequisitePrompts>
      <RandomData>
        <DataElement DataList="LocationTypes" number ="1" repeatable="true" returnName="LocationType"/>
      </RandomData>
    </PromptLine>
  </prompt>
  <prompt name="Quest">
    <ValidModifiers value=""/>
    <PromptLine id="1" value="Generate a Dungeons and Dragons quest that starts in a @LocationType@ and has a hook, a twist, and a resolution. Also describe @number@ characters.">
      <PrerequisitePrompts>
      </PrerequisitePrompts>
      <RandomData>
        <DataElement DataList="LocationTypes" number ="1" repeatable="true" returnName="LocationType"/>
        <DataElement DataList="Numbers" number ="1" repeatable="true" returnName="number"/>
      </RandomData>
    </PromptLine>
  </prompt>
  <prompt name ="NPC">
    <ValidModifiers value="Condencsed"/>
    <PromptLine id="1" value="Generate a random fantasy character description. Their race is @race@.  They are of class @class@.">
      <PrerequisitePrompts>
        <PrereqPrompt prompt="Name" returnName="name" number="1"/>
      </PrerequisitePrompts>
      <RandomData>
        <DataElement DataList="Races" number ="1" repeatable="false" returnName="race"/>
        <DataElement DataList="Classes" number ="1" repeatable="false" returnName="class"/>
      </RandomData>
    </PromptLine>
  </prompt>
  <prompt name ="Name">
    <ValidModifiers value=""/>
    <PromptLine id="1" value="Generate a random @gender@ @race@ fantasy name. Only display the name.">
      <PrerequisitePrompts>
      </PrerequisitePrompts>
      <RandomData>
        <DataElement DataList="Gender" number ="1" repeatable="false" returnName="gender"/>
        <DataElement DataList="Races" number ="1" repeatable="false" returnName="race"/>
      </RandomData>
    </PromptLine>
  </prompt>
  <prompt name="Location">
    <ValidModifiers value="TestModifier,TestModifier2"/>
    <PromptLine id="1" filter="" value="Generate a fantasy @climate@ location. Give a brief history of the area.">
      <PrerequisitePrompts>
      </PrerequisitePrompts>
      <RandomData>
        <DataElement DataList="Climate" number ="1" repeatable="false" returnName="climate"/>
      </RandomData>
    </PromptLine>
  </prompt>
  <prompt name="Number">
    <ValidModifiers value="TestModifier,TestModifier2"/>
    <PromptLine id="1" filter="" value="">
      <PrerequisitePrompts>
        <PrereqPrompt prompt="" returnName="" number=""/>
      </PrerequisitePrompts>
      <RandomData>
        <DataElement DataList="" number ="" repeatable="" returnName=""/>
      </RandomData>
    </PromptLine>
  </prompt>
</Prompts>
